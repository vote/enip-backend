{
  "$id": "https://schema.voteamerica.com/enip/national.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ENIP National Data Schema",
  "type": "object",
  "definitions": {
    "winner": {
      "$id": "#winner",
      "oneOf": [
        {"type": "string", "enum": ["dem", "gop", "oth"]},
        {"type": "null"}
      ]
    },
    "comments": {
      "$id": "#comments",
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "timestamp",
          "author",
          "title",
          "body"
        ],
        "properties": {
          "timestamp": {
            "type": "string",
            "format": "datetime"
          },
          "author": { "type": "string" },
          "title": { "type": "string" },
          "body": { "type": "string" }
        }
      }
    },
    "national_summary_p_candidate_named": {
      "$id": "#national_summary_p_candidate_named",
      "type": "object",
      "additionalProperties": false,
      "required": [
        "firstName",
        "lastName",
        "popVote",
        "popPct",
        "statesWon",
        "electWon"
      ],
      "properties": {
        "firstName": {"type": "string"},
        "lastName": {"type": "string"},
        "popVote": {"type": "integer"},
        "popPct": {"type": "number"},
        "statesWon": {"type": "integer"},
        "electWon": {"type": "integer"}
      }
    },
    "national_summary_p_candidate_unnamed": {
      "$id": "#national_summary_p_candidate_unnamed",
      "type": "object",
      "additionalProperties": false,
      "required": [
        "popVote",
        "popPct",
        "statesWon",
        "electWon"
      ],
      "properties": {
        "popVote": {"type": "integer"},
        "popPct": {"type": "number"},
        "statesWon": {"type": "integer"},
        "electWon": {"type": "integer"}
      }
    },
    "national_summary_p": {
      "$id": "#national_summary_p",
      "type": "object",
      "additionalProperties": false,
      "required": [
        "dem",
        "gop",
        "oth",
        "winner"
      ],
      "properties": {
        "dem":  {"$ref": "#/definitions/national_summary_p_candidate_named"},
        "gop":  {"$ref": "#/definitions/national_summary_p_candidate_named"},
        "oth":  {"$ref": "#/definitions/national_summary_p_candidate_unnamed"},
        "winner": {"$ref": "#/definitions/winner"}
      }
    },
    "national_summary_winner_count": {
      "$id": "#national_summary_winner_count",
      "type": "object",
      "additionalProperties": false,
      "required": ["dem", "gop", "oth"],
      "properties": {
        "dem": {
          "type": "object",
          "additionalProperties": false,
          "required": ["won"],
          "properties": {
            "won": { "type": "integer" }
          }
        },
        "gop": {
          "type": "object",
          "additionalProperties": false,
          "required": ["won"],
          "properties": {
            "won": { "type": "integer" }
          }
        },
        "oth": {
          "type": "object",
          "additionalProperties": false,
          "required": ["won"],
          "properties": {
            "won": { "type": "integer" }
          }
        }
      }
    },
    "state_summary_candidate_named": {
      "$id": "#state_summary_candidate_named",
      "type": "object",
      "additionalProperties": false,
      "required": [
        "firstName",
        "lastName",
        "popVote",
        "popPct",
        "popVoteHistory"
      ],
      "properties": {
        "firstName": {"type": "string"},
        "lastName": {"type": "string"},
        "popVote": {"type": "integer"},
        "popPct": {"type": "number"},
        "popVoteHistory": {
          "type": "object",
          "propertyNames": {
            "type": "string",
            "format": "datetime"
          },
          "additionalProperties": {
            "type": "integer"
          }
        }
      }
    },
    "state_summary_candidate_unnamed": {
      "$id": "#state_summary_candidate_unnamed",
      "type": "object",
      "additionalProperties": false,
      "required": [
        "popVote",
        "popPct",
        "popVoteHistory"
      ],
      "properties": {
        "popVote": {"type": "integer"},
        "popPct": {"type": "number"},
        "popVoteHistory": {
          "type": "object",
          "propertyNames": {
            "type": "string",
            "format": "datetime"
          },
          "additionalProperties": {
            "type": "integer"
          }
        }
      }
    },
    "state_summary_p": {
      "$id": "#state_summary_p",
      "type": "object",
      "additionalProperties": false,
      "required": [
        "dem",
        "gop",
        "oth",
        "winner",
        "comments"
      ],
      "properties": {
        "dem": {"$ref": "#/definitions/state_summary_candidate_named" },
        "gop": {"$ref": "#/definitions/state_summary_candidate_named" },
        "oth": {"$ref": "#/definitions/state_summary_candidate_unnamed" },
        "winner": {"$ref": "#/definitions/winner"},
        "comments": {"$ref": "#/definitions/comments"}
      }
    },
    "state_summary_congressional_result": {
      "$id": "#state_summary_congressional_result",
      "type": "object",
      "additionalProperties": false,
      "required": ["oth", "multipleDem", "multipleGop", "comments"],
      "properties": {
        "dem": {"$ref": "#/definitions/state_summary_candidate_named" },
        "gop": {"$ref": "#/definitions/state_summary_candidate_named" },
        "oth": {"$ref": "#/definitions/state_summary_candidate_unnamed" },
        "multipleDem": {"type": "boolean"},
        "multipleGop": {"type": "boolean"},
        "winner": {"$ref": "#/definitions/winner"},
        "comments": {"$ref": "#/definitions/comments"}
      }
    },
    "state_summary": {
      "$id": "#state_summary",
      "type": "object",
      "additionalProperties": false,
      "required": ["P", "H", "S"],
      "properties": {
        "P": {
          "$ref": "#/definitions/state_summary_p"
        },
        "S": {
          "$ref": "#/definitions/state_summary_congressional_result"
        },
        "H": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/state_summary_congressional_result"
          },
          "propertyNames": {
            "pattern": "^(\\d\\d)|(AL)$"
          }
        }
      }
    },
    "presidential_cd_summary": {
      "$id": "#presidential_cd_summary",
      "type": "object",
      "additionalProperties": false,
      "required": ["P"],
      "properties": {
        "P": {
          "$ref": "#/definitions/state_summary_p"
        }
      }
    },
    "senate_special_summary": {
      "$id": "#senate_special_summary",
      "type": "object",
      "additionalProperties": false,
      "required": ["S"],
      "properties": {
        "S": {
          "$ref": "#/definitions/state_summary_congressional_result"
        }
      }
    }
  },
  "properties": {
    "national_summary": {
      "type": "object",
      "description": "National summary",
      "additionalProperties": false,
      "required": ["P", "H", "S"],
      "properties": {
        "P": {
